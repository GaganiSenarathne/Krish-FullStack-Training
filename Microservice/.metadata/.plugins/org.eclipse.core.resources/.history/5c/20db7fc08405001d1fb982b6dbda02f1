  endpoints:
    web:
      exposure:
        include: '*'
        
spring:
  datasource:
    url: jdbc:mysql://localhost:3306/spring_boot
    username: root
    password: 
    driver-class-name: com.mysql.jdbc.Driver

  main: 
    web-application-type: none
    
  jpa:
    defer-datasource-initialization: true