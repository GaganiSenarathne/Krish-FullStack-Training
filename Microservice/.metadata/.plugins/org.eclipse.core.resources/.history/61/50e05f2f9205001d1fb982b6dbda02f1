  datasource:
    url: jdbc:mysql://localhost:3306/spring_boot_auto?createdatabaseifnotexist=true
    username: root
    password: 
    driver-class-name: com.mysql.cj.jdbc.Driver
    
  jpa:
    defer-datasource-initialization: true